/* 搜索框 */
.search {
  padding: 25rpx;
  background-color: #fff;
}

.s_input {
  height: 50rpx;
  font-size: 30rpx;
  text-align: center;
  background-color: #f3f3f3;
  border-radius: 25rpx;
}

/* 标题 */
.top {
  height: 40rpx;
  padding: 20rpx 30rpx;
  background-color: #fff;
  border-bottom: 1rpx solid #f3f3f3;
}

.top_title {
  border-left: 5rpx solid #29a03c;
  padding-left: 20rpx;
  font-size: 30rpx;
}